{
  "manifest_version": 3,
  "name": "Claude Agent Hackathon",
  "version": "1.0",
  "description": "A simple test extension for the hackathon project.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "Photo on 10-26-25 at 1.11 PM.jpg",
      "48": "Photo on 10-26-25 at 1.11 PM.jpg",
      "128": "Photo on 10-26-25 at 1.11 PM.jpg"
    }
  },
  "permissions": ["scripting", "tabs"]
  "host_permissions": ["<all_urls>"]
}
